<Window x:Class="lokqlDx.PreferencesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lokqlDx"
        mc:Ignorable="d"
        Title="PreferencesWindow" Height="450" Width="800"
        Loaded="PreferencesWindow_OnLoaded">
  <DockPanel>
    <StackPanel Orientation="Horizontal" DockPanel.Dock="Bottom" HorizontalAlignment="Right">
      <Button IsCancel="True">Cancel</Button>
      <Button Click="OnOk">Ok</Button>
    </StackPanel>
    <DockPanel>
      <Label Content="Pre-script (run on load of all workspaces)" DockPanel.Dock="Top"></Label>
      <TextBox Name="StartupScript" AcceptsReturn="True" Background="Black" Foreground="White"></TextBox>
    </DockPanel>

  </DockPanel>

</Window>
